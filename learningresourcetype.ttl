    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Audio"@de, "Audio"@en ;
    skos:altLabel "Tonaufnahme"@de ;
    skos:altLabel "audio recording"@en ;
    skos:definition "Eine Tonaufnahme zum Anhören."@de ;
    skos:definition "An audio recording to listen to."@en ;
    skos:example "Musikstück, Radioarchive, aufgenommene Stimmen und Töne etc."@de ;
    skos:example "English translation pending."@en .

lrt:AUDIOVISUELLES a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Audiovisuelles Medium"@de, "Audiovisual medium"@en ;
    skos:definition "Ein Bild- und Tondokument auf einem physischen Datenträger."@de ;
    skos:definition "Physical medium containing audio and video."@en ;
    skos:example "DVD, Blu-ray Disc"@de ;
    skos:example "DVD, Blu-ray Disc"@en .

lrt:BILD a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Bild"@de, "Image"@en ;
    skos:definition "Alle Arten von unbewegten Bildern oder grafischen Darstellungen."@de ;
    skos:definition "All kinds of still images or graphical representations."@en ;
    skos:example "Foto, Grafiken, Illustration, Artefakt eines Gemäldes, Schema, Diagramm etc."@de ;
    skos:example "English translation pending."@en .

lrt:BROSCHUERE a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Broschüre"@de, "Brochure"@en ;
    skos:definition "Eine mehrseitige textliche Zusammenfassung, die, ausführlicher als beim Flyer, Auskünfte zu einem Thema gibt und die Leser und Leserinnen informiert."@de ;
    skos:definition "English translation pending."@en ;
    skos:example "Broschüre, Sicherheitshinweise, Sachinformationen"@de ;
    skos:example "English translation pending."@en .

lrt:DATEN a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Daten"@de, "Data"@en ;
    skos:definition "Empirische Daten, Fakten und Erhebungen, Rohdaten."@de ;
    skos:definition "English translation pending."@en ;
    skos:example "Geschichtsdaten, Erhebungen, Fakten, Werte, Statistiken, Steckbriefe"@de .

lrt:EBOOK a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "E-Book"@de, "E-book"@en ;
    skos:definition "E-Book steht für ein elektronisches Buch und bezeichnet Werke in elektronischer Buchform, die auf E-Book-Readern oder mit spezieller Software auf PCs, Tabletcomputern oder Smartphones gelesen werden können. Interaktive Inhalte sorgen beispielsweise dafür, dass Schülerinnen und Schüler Aufgaben direkt im Buch bearbeiten und gemeinsam an Projekten arbeiten können. Auch die Resonanz der Lehrperson kann hierbei eingebunden werden."@de ;
    skos:definition "English translation pending."@en ;
    skos:example "Interaktives Lehrbuch, Interaktives Übungsbuch"@de .

lrt:ENTDECKENDES a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Entdeckendes Lernen"@de, "Discovery Learning"@en ;
    skos:definition "Durch virtuelle und reale Handlungen werden neue Dinge entdeckt und gelernt. Der Entdeckergeist und die Neugier werden geweckt und das Lernen ist hierbei der positive Nebeneffekt."@de ;
    skos:definition "Discovery learning is a technique of inquiry-based learning and is considered a constructivist based approach to education. It is also referred to as problem-based learning, experiential learning and 21st century learning."@en ;
    skos:example "Spielerische Lernaufträge in der Natur oder in digitalen Räumen, in denen durch Klicken auf Symbole neue Dinge und Fakten entdeckt werden"@de .

lrt:EXPERIMENT a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Experiment"@de, "Experiment"@en ;
    skos:definition "Wissenschaftlicher Versuch mit einer klaren Methodik, bei der Handlungen und Prozeduren erprobt werden, um anhand von Messungen oder Beobachtungen eine Hypothese zu überprüfen oder bereits Bekanntes zu belegen oder darzustellen."@de ;
    skos:definition "English translation pending"@en ;
    skos:example "Video eines Experiments, das im Chemieunterricht nachgestellt wird etc."@de .

lrt:FALLSTUDIE a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Fallstudie"@de, "Case Study"@en ;
    skos:definition "Untersuchung einer bestimmten Situation, einer Person oder eines Themas."@de ;
    skos:definition "A case study is an in-depth, detailed examination of a particular case (or cases) within a real-world context."@en ;
    skos:example "Präsentation zum pädagogischen Einsatz von Tablets an schweizerischen Primarschulen, Analyse der Konfliktsituation in einem Unternehmen etc."@de .

lrt:FLYER a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Flyer"@de, "Flyer"@en ;
    skos:definition "Eine stichpunktartige Kurzzusammenfassung, die die wichtigsten Eckpunkte zu einem Thema gibt und die Leser und Leserinnen informiert. Einen Flyer ist ein einziges Blatt, meist A4, oft gefaltet."@de ;
    skos:example "Info-Flyer, Werbe-Flyer, Faltblatt"@de .

lrt:GLOSSAR a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Glossar"@de, "Glossary"@en ;
    skos:altLabel "Nachschlagewerk"@de ;
    skos:altLabel "Reference book"@en ;
    skos:definition "Alphabetisch oder thematisch sortierte Liste von Wörtern mit beigefügten Bedeutungserklärungen oder Übersetzungen."@de ;
    skos:definition "English translation pending."@en ;
    skos:example "Begriffsverzeichnis, Fachglossar der holzverarbeitenden Berufe, Wörterbuch, Enzyklopädie"@de .

lrt:HANDBUCH a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Handbuch"@de, "Guide"@en ;
    skos:definition "Ein Handbuch ist eine umfangreiche, strukturierte Textform, in den Vorschläge und Vorgehensweisen unterbreitet werden, mit Situationen oder Personen umzugehen. Hierbei liefert es Methoden zur Umsetzung, die man immer wieder nachschlagen kann."@de ;
    skos:definition "English translation pending."@en ;
    skos:example "Handbuch, Verhaltenstipps, Methodenvorschläge"@de ;
    skos:example "English translation pending."@en .

lrt:HANDREICHUNG a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Handreichung"@de, "Guidance"@en ;
    skos:definition "Spezifisch für Lehrkräfte gedachte didaktische Hinweise, wie ein bestimmtes Thema praktisch umgesetzt werden kann. Eine Handreichung kann auch Lernsituationen und Arbeitsblätter enthalten."@de ;
    skos:definition "English translation pending."@en ;
    skos:example "Handreichung zur Integration digitaler Schlüsselkompetenzen in die Berufliche Bildung, Handreichung Sprachsensibler Unterricht an Berufsschulen"@de ;
    skos:example "English translation pending."@en .

lrt:INFOGRAFIK a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Infografik"@de, "Infographic"@en ;
    skos:definition "Eine Grafik, die komplexe Zusammenhänge und umfangreiches Datenmaterial anschaulich aufbereitet. Infografiken helfen dem Rezipienten, die Daten einfach und schnell zu erfassen, zu verstehen und zu interpretieren."@de ;
    skos:definition "English translation pending."@en ;
    skos:example "Übersicht zum Kreislauf von Pfandflaschen, Erklär-Grafik zur Lebensmittelkennzeichnung: Was steht auf dem Etikett?"@de ;
    skos:example "English translation pending."@en .

lrt:INTERAKTION a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Interaktion"@de, "Interaction"@en ;
    skos:definition "Eine Interaktion ist das Interagieren mit einem Programm. Es ist eine Wechselbeziehung zwischen Programm und Anwender, in der eine Reaktion auf eine Aktion folgt. "@de ;
    skos:definition "English translation pending."@en ;
    skos:example "Drag-and-Drop-Rätsel, Zuordnungen, interaktive Rechenaufgaben, H5P"@de ;
    skos:example "English translation pending."@en .

lrt:KARTE a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Karte"@de, "Map"@en ;
    skos:definition "Darstellung von raumbezogenen Gegenständen, Sachverhalten oder Prozessen, die maßstäblich generalisiert und mithilfe eines Zeichensystems grafisch gezeigt werden."@de ;
    skos:definition "English translation pending."@en ;
    skos:example "Geografische Karte zur Alpenkette, Karte zu Bayern mit den Regierungsbezirken"@de ;
    skos:example "English translation pending."@en .

lrt:KURS a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Kurs"@de, "Course"@en ;
    skos:definition "Ein Kurs besteht aus mehreren Unterrichtseinheiten und beschäftigt sich mit einem Thema über einen gewissen Zeitraum. "@de ;
    skos:definition "English translation pending."@en ;
    skos:example "Studienkurs, Wahlkurs, Onlinekurs, Sprachkurs"@de ;
    skos:example "English translation pending."@en .

lrt:LEHRBUCH a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:prefLabel "Lehrbuch"@de, "Textbook"@en ;
