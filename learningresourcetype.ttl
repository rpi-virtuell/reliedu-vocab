    skos:definition "English translation pending."@en ;
    skos:example "Selbsttest, Lernstandsanalyse"@de ;
    skos:example "English translation pending."@en .

lrt:LERNSPIEL a skos:Concept ;
    skos:inScheme <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
    skos:topConceptOf <https://vocabs.sodix.de/sodix/educational/learningresourcetype/learningresourcetype> ;
