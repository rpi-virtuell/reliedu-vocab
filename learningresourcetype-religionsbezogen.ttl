@base <https://w3id.org/kim/lrt-religion/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix <https://vocabs.sodix.de/sodix/educational/learningresourcetype/> <https://vocabs.sodix.de/sodix/educational/learningresourcetype/> .
@prefix <https://vocabs.openeduhub.de/w3id.org/openeduhub/vocabs/new_lrt/> <https://vocabs.openeduhub.de/w3id.org/openeduhub/vocabs/new_lrt/> .
@prefix <https://w3id.org/kim/hcrt/> <https://w3id.org/kim/hcrt/> .
@prefix <https://bokahama.github.io/SCRP-Vocab-LRT/> <https://bokahama.github.io/SCRP-Vocab-LRT/> .

<scheme> a skos:ConceptScheme ;
    dct:title "Lernressourcentypen für religionsbezogene Bildung"@de ;
    dct:title "Learning Resource Types for Religious Education"@en ;
    dct:description "Eine erweiterte und integrierte Werteliste für Typen von Lernressourcen im Kontext religionsbezogener und religionspädagogischer Bildung. Diese Liste vereint Elemente aus dem HCRT (Higher Education), SODIX und SCRP-Vokabularen und erweitert sie um spezifische Konzepte für den religiösen Bildungskontext."@de ;
    dct:description "An extended and integrated value list for Learning Resource Types in the context of religious and religious-pedagogical education. This list combines elements from HCRT (Higher Education), SODIX and SCRP vocabularies and extends them with specific concepts for religious educational contexts."@en ;
    dct:creator "Jörg Lohrer"@de ;
    dct:created "2026-01-29"^^xsd:date ;
    dct:modified "2026-01-29"^^xsd:date ;
    dct:issued "2026-01-29"^^xsd:date ;
    vann:preferredNamespaceUri "https://w3id.org/kim/lrt-religion/" ;
    vann:preferredNamespacePrefix "lrtreligion" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    skos:hasTopConcept 
        <unterrichtsplanung>, 
        <methode>, 
        <app>, 
        <aufgabe_uebung>, 
        <bild>, 
        <digitales_lernmittel>, 
        <interaktion>, 
        <audio>, 
        <notation>, 
        <video>, 
        <religioeser_quellentext>,
        <religioeser_praxisbezug>,
        <sonstiges> .

# ========================================
# UNTERRICHTSPLANUNG (Top-Level)
# ========================================

<unterrichtsplanung> a skos:Concept ;
    skos:topConceptOf <scheme> ;
    skos:prefLabel "Unterrichtsplanung"@de, "Lesson Planning"@en ;
    skos:definition "Alle didaktischen und pädagogischen Überlegungen zur Vorbereitung einer Unterrichtsstunde oder Unterrichtseinheit im religionspädagogischen Kontext."@de ;
    skos:definition "All didactic and pedagogical considerations for preparing a lesson or teaching unit in the context of religious education."@en ;
    skos:example "Unterrichtsentwurf zum Thema 'Gleichnisse Jesu', Verlaufsplanung einer Einheit zu 'Weltreligionen'"@de ;
    skos:narrower <unterrichtsbaustein>, <unterrichtsidee>, <lehrplanreferenz>, <handreichung>, <lernkontrolle>, <loesung>, <test>, <selbstevaluation> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/lesson_plan>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/UNTERRICHTSPLANUNG>, <https://bokahama.github.io/SCRP-Vocab-LRT/UNTERRICHTSPLANUNG> ;
    skos:inScheme <scheme> .

<unterrichtsbaustein> a skos:Concept ;
    skos:prefLabel "Unterrichtsbaustein"@de, "Teaching Module"@en ;
    skos:definition "Eine kleinere, eigenständige Einheit im Rahmen eines Lehrplans oder einer Unterrichtsstruktur im Religionsunterricht, die aus didaktischen Hinweisen, Beispielen, Texten, Materialien und/oder Aufgaben besteht."@de ;
    skos:example "Unterrichtsmodul 'Schöpfungsglaube und Naturwissenschaft', Themenblock 'Propheten im AT'"@de ;
    skos:broader <unterrichtsplanung> ;
    skos:exactMatch <https://vocabs.sodix.de/sodix/educational/learningresourcetype/UNTERRICHTSBAUSTEIN>, <https://bokahama.github.io/SCRP-Vocab-LRT/UNTERRICHTSBAUSTEIN> ;
    skos:inScheme <scheme> .

<unterrichtsidee> a skos:Concept ;
    skos:prefLabel "Unterrichtsidee"@de, "Teaching Idea"@en ;
    skos:definition "Eine kreative und innovative Methode oder Aktivität, die Lehrkräfte im Religionsunterricht einsetzen können, um das Lernen zu fördern und zu bereichern."@de ;
    skos:example "Kreative Gestaltung eines Gebetswürfels, Biblische Geschichten als Standbilder darstellen"@de ;
    skos:broader <unterrichtsplanung> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/UNTERRICHTSIDEE> ;
    skos:inScheme <scheme> .

<lehrplanreferenz> a skos:Concept ;
    skos:prefLabel "Lehrplanreferenz"@de, "Curriculum Reference"@en ;
    skos:definition "Dokument oder Ressource mit Bezug auf den Lehrplan für Religionsunterricht, mit detaillierten Informationen über Inhalte, Ziele und Methoden."@de ;
    skos:example "Lehrplan für katholischen Religionsunterricht Klasse 5-6, Bildungsplan evangelische Religion Sek I"@de ;
    skos:broader <unterrichtsplanung> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/LEHRPLANREFERENZ> ;
    skos:inScheme <scheme> .

<handreichung> a skos:Concept ;
    skos:prefLabel "Handreichung"@de, "Guidance"@en ;
    skos:definition "Spezifisch für Lehrkräfte im Religionsunterricht gedachte didaktische Hinweise, wie ein bestimmtes religiöses Thema praktisch umgesetzt werden kann."@de ;
    skos:example "Handreichung 'Interreligiöses Lernen im Klassenzimmer', Handreichung 'Umgang mit religiöser Vielfalt'"@de ;
    skos:broader <unterrichtsplanung> ;
    skos:exactMatch <https://vocabs.sodix.de/sodix/educational/learningresourcetype/HANDREICHUNG>, <https://bokahama.github.io/SCRP-Vocab-LRT/HANDREICHUNG> ;
    skos:inScheme <scheme> .

<lernkontrolle> a skos:Concept ;
    skos:prefLabel "Lernkontrolle"@de, "Learning Assessment"@en ;
    skos:altLabel "Lernstandserhebung"@de ;
    skos:definition "Verschiedene Methoden zur Überprüfung des Lernfortschritts im Religionsunterricht, formell oder informell, bewertet oder unbewertet."@de ;
    skos:example "Selbsttest zu 'Die zehn Gebote', Lernstandsanalyse 'Religiöse Symbole'"@de ;
    skos:broader <unterrichtsplanung> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/assessment>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/LERNKONTROLLE>, <https://bokahama.github.io/SCRP-Vocab-LRT/LERNKONTROLLE> ;
    skos:inScheme <scheme> .

<loesung> a skos:Concept ;
    skos:prefLabel "Lösung"@de, "Solution"@en ;
    skos:altLabel "Lösungsblatt"@de ;
    skos:definition "Dokument mit korrekten Antworten oder Lösungsschritten zu Aufgaben im Religionsunterricht."@de ;
    skos:example "Lösungsblatt zu Bibel-Quiz, Musterlösung zur Textanalyse einer Perikope"@de ;
    skos:broader <unterrichtsplanung> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/LOESUNG> ;
    skos:inScheme <scheme> .

<test> a skos:Concept ;
    skos:prefLabel "Prüfung"@de, "Examination"@en ;
    skos:altLabel "Test (bewertet)"@de, "Klassenarbeit"@de ;
    skos:definition "Bewertete Lernkontrolle im Religionsunterricht, deren Ergebnis in die Benotung einfließt."@de ;
    skos:example "Klassenarbeit 'Christologie', Test 'Weltreligionen im Vergleich'"@de ;
    skos:broader <unterrichtsplanung> ;
    skos:exactMatch <https://vocabs.sodix.de/sodix/educational/learningresourcetype/TEST>, <https://bokahama.github.io/SCRP-Vocab-LRT/TEST> ;
    skos:inScheme <scheme> .

<selbstevaluation> a skos:Concept ;
    skos:prefLabel "Selbstevaluation"@de, "Self-Evaluation"@en ;
    skos:definition "Übung zur Selbstbewertung im Religionsunterricht, bei der Lernende ihre eigenen Leistungen und religiösen Lernprozesse reflektieren."@de ;
    skos:example "Selbsteinschätzungsbogen 'Meine religiöse Entwicklung', Reflexionsübung zu ethischen Dilemmata"@de ;
    skos:broader <unterrichtsplanung> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/SELBSTEVALUATION> ;
    skos:inScheme <scheme> .

# ========================================
# METHODE (Top-Level)
# ========================================

<methode> a skos:Concept ;
    skos:topConceptOf <scheme> ;
    skos:prefLabel "Pädagogische Methode"@de, "Pedagogical Method"@en ;
    skos:altLabel "Methodische Ansätze"@de ;
    skos:definition "Verschiedene Lehrmethoden und methodische Konzepte für den Religionsunterricht, die nicht primär medial sind, sondern didaktische Arrangements beschreiben."@de ;
    skos:example "Biblisches Bibliodrama, Theologisieren mit Kindern"@de ;
    skos:narrower <barcamp>, <entdeckendes_lernen>, <experiment>, <experte>, <offene_aktivitaet>, <gruppenarbeit>, <lernort>, <fallstudie>, <projekt>, <rollenspiel>, <sole>, <stationenlernen>, <religioeser_dialog>, <meditation_stille> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/METHODE> ;
    skos:inScheme <scheme> .

<barcamp> a skos:Concept ;
    skos:prefLabel "Barcamp"@de, "Barcamp"@en ;
    skos:definition "Offene Tagung mit Workshops im religionspädagogischen Kontext, deren Inhalte von Teilnehmenden selbst entwickelt werden."@de ;
    skos:example "Religions-EduCamp, Theologisches Barcamp"@de ;
    skos:broader <methode> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/BARCAMP> ;
    skos:inScheme <scheme> .

<entdeckendes_lernen> a skos:Concept ;
    skos:prefLabel "Entdeckendes Lernen"@de, "Discovery Learning"@en ;
    skos:definition "Durch eigene Erkundung werden religiöse Inhalte und spirituelle Erfahrungen entdeckt und gelernt."@de ;
    skos:example "Kirchenraumpädagogische Erkundung, Entdeckendes Lernen mit religiösen Symbolen"@de ;
    skos:broader <methode> ;
    skos:exactMatch <https://vocabs.sodix.de/sodix/educational/learningresourcetype/ENTDECKENDES>, <https://bokahama.github.io/SCRP-Vocab-LRT/ENTDECKENDES_LERNEN> ;
    skos:inScheme <scheme> .

<experiment> a skos:Concept ;
    skos:prefLabel "Experiment"@de, "Experiment"@en ;
    skos:definition "Wissenschaftlicher Versuch oder erfahrungsbezogenes Experiment im Religionsunterricht."@de ;
    skos:example "Soziales Experiment zu 'Nächstenliebe', Wahrnehmungsexperiment zu 'Stille und Gebet'"@de ;
    skos:broader <methode> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/experiment>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/EXPERIMENT>, <https://bokahama.github.io/SCRP-Vocab-LRT/EXPERIMENT> ;
    skos:inScheme <scheme> .

<experte> a skos:Concept ;
    skos:prefLabel "Experte/Referent"@de, "Expert/Speaker"@en ;
    skos:altLabel "Zeitzeugin"@de, "Religionsvertreter"@de ;
    skos:definition "Personen mit besonderem Wissen oder religiöser Expertise, die im Religionsunterricht als Referent:innen oder Expert:innen eingebunden werden."@de ;
    skos:example "Rabbinerin als Expertin für jüdisches Leben, Theologieprofessor als Referent, Ordensfrau als Zeitzeugin"@de ;
    skos:broader <methode> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/EXPERTE> ;
    skos:inScheme <scheme> .

<offene_aktivitaet> a skos:Concept ;
    skos:prefLabel "Offene Aktivität"@de, "Open Activity"@en ;
    skos:definition "Freie, nicht eng strukturierte Lernaktivität im Religionsunterricht."@de ;
    skos:example "Freies Philosophieren über Glaubensfragen, offene Gesprächsrunde"@de ;
    skos:broader <methode> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/OFFENE_AKTIVITAET> ;
    skos:inScheme <scheme> .

<gruppenarbeit> a skos:Concept ;
    skos:prefLabel "Gruppenarbeit"@de, "Group Work"@en ;
    skos:definition "Kooperative Lernmethode, bei der Lernende in Gruppen religiöse Themen erarbeiten."@de ;
    skos:example "Gruppenarbeit zu verschiedenen Weltreligionen, Team-Projekt 'Schöpfung und Verantwortung'"@de ;
    skos:broader <methode> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/GRUPPENARBEIT> ;
    skos:inScheme <scheme> .

<lernort> a skos:Concept ;
    skos:prefLabel "Lernort"@de, "Learning Location"@en ;
    skos:altLabel "Exkursion"@de, "Außerschulischer Lernort"@de ;
    skos:definition "Orte außerhalb des Klassenzimmers, die religiöses Lernen durch direkte Erfahrung ermöglichen."@de ;
    skos:example "Besuch einer Synagoge, Exkursion zum Kloster, Friedhofserkundung"@de ;
    skos:broader <methode> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/LERNORT> ;
    skos:inScheme <scheme> .

<fallstudie> a skos:Concept ;
    skos:prefLabel "Fallstudie"@de, "Case Study"@en ;
    skos:definition "Detaillierte Untersuchung eines spezifischen Falls oder einer Situation im Religionsunterricht."@de ;
    skos:example "Fallstudie zu ethischen Dilemmata, Analyse eines Konflikts zwischen religiösen Werten"@de ;
    skos:broader <methode> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/case_study>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/FALLSTUDIE>, <https://bokahama.github.io/SCRP-Vocab-LRT/FALLSTUDIE> ;
    skos:inScheme <scheme> .

<projekt> a skos:Concept ;
    skos:prefLabel "Projekt"@de, "Project"@en ;
    skos:altLabel "Projektarbeit"@de ;
    skos:definition "Längerfristige, zielorientierte Arbeit zu einem religiösen oder ethischen Thema in Einzel- oder Gruppenarbeit."@de ;
    skos:example "Projekt 'Religionen in unserer Stadt', Sozialprojekt 'Diakonie erleben'"@de ;
    skos:broader <methode> ;
    skos:exactMatch <https://vocabs.sodix.de/sodix/educational/learningresourcetype/PROJECT>, <https://bokahama.github.io/SCRP-Vocab-LRT/PROJEKT> ;
    skos:inScheme <scheme> .

<rollenspiel> a skos:Concept ;
    skos:prefLabel "Rollenspiel"@de, "Role Play"@en ;
    skos:altLabel "Szenisches Spiel"@de, "Bibliodrama"@de ;
    skos:definition "Spiel, bei dem Lernende in religiöse oder ethische Rollen schlüpfen und Situationen nachspielen."@de ;
    skos:example "Rollenspiel 'Streitgespräch der Propheten', Bibliodrama zu Gleichnissen Jesu"@de ;
    skos:broader <methode> ;
    skos:exactMatch <https://vocabs.sodix.de/sodix/educational/learningresourcetype/ROLLENSPIEL>, <https://bokahama.github.io/SCRP-Vocab-LRT/ROLLENSPIEL> ;
    skos:inScheme <scheme> .

<sole> a skos:Concept ;
    skos:prefLabel "Selbstorganisiertes Lernen"@de, "Self-Organized Learning"@en ;
    skos:definition "Lernmethode, bei der Lernende selbstständig ihre religiösen Lernprozesse planen und reflektieren."@de ;
    skos:example "Selbstgesteuertes Projekt zu 'Meine spirituelle Reise'"@de ;
    skos:broader <methode> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/SOLE> ;
    skos:inScheme <scheme> .

<stationenlernen> a skos:Concept ;
    skos:prefLabel "Stationenlernen"@de, "Station Learning"@en ;
    skos:altLabel "Lernzirkel"@de ;
    skos:definition "Unterrichtsmethode mit verschiedenen Stationen zu religiösen Themen, die in Kleingruppen bearbeitet werden."@de ;
    skos:example "Stationenlernen 'Die Sakramente', Lernzirkel 'Weltreligionen kennenlernen'"@de ;
    skos:broader <methode> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/STATIONENLERNEN> ;
    skos:inScheme <scheme> .

<religioeser_dialog> a skos:Concept ;
    skos:prefLabel "Religiöser Dialog"@de, "Religious Dialogue"@en ;
    skos:altLabel "Interreligiöser Dialog"@de, "Trialog"@de ;
    skos:definition "Strukturiertes Gespräch zwischen Angehörigen verschiedener religiöser Traditionen oder über religiöse Fragen."@de ;
    skos:example "Trialog zwischen Judentum, Christentum und Islam, Philosophisches Gespräch über Gottesfrage"@de ;
    skos:broader <methode> ;
    skos:inScheme <scheme> .

<meditation_stille> a skos:Concept ;
    skos:prefLabel "Meditation und Stille"@de, "Meditation and Silence"@en ;
    skos:altLabel "Stilleübung"@de, "Achtsamkeitsübung"@de ;
    skos:definition "Übungen der Stille, Meditation oder Achtsamkeit im Religionsunterricht zur Förderung von Spiritualität und Selbstreflexion."@de ;
    skos:example "Geführte Meditation zu biblischen Texten, Stilleübung vor dem Gebet, Achtsamkeitsübung"@de ;
    skos:broader <methode> ;
    skos:inScheme <scheme> .

# ========================================
# APP (Top-Level)
# ========================================

<app> a skos:Concept ;
    skos:topConceptOf <scheme> ;
    skos:prefLabel "App"@de, "App"@en ;
    skos:altLabel "Anwendung"@de, "Software"@de ;
    skos:definition "Softwareprogramm für Computer oder mobile Endgeräte im Kontext religionsbezogener Bildung."@de ;
    skos:example "Bibel-App, App zum Erlernen religiöser Feste, Gebetszeiten-App"@de ;
    skos:narrower <lernspiel>, <methodische_app>, <software>, <webtool> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/application>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/APP>, <https://bokahama.github.io/SCRP-Vocab-LRT/APP> ;
    skos:inScheme <scheme> .

<lernspiel> a skos:Concept ;
    skos:prefLabel "Lernspiel"@de, "Educational Game"@en ;
    skos:definition "Spielerisches Lernmaterial zu religiösen Inhalten nach definierten Regeln."@de ;
    skos:example "Bibel-Quiz-Spiel, Memory mit religiösen Symbolen, Rätsel zu Weltreligionen"@de ;
    skos:broader <app> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/educational_game>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/LERNSPIEL>, <https://bokahama.github.io/SCRP-Vocab-LRT/LERNSPIEL> ;
    skos:inScheme <scheme> .

<methodische_app> a skos:Concept ;
    skos:prefLabel "Lern-APP"@de, "Learning APP"@en ;
    skos:altLabel "Methodische APP"@de ;
    skos:definition "Anwendung oder Programm zur Vermittlung religiöser Lerninhalte oder zur Kompetenzentwicklung."@de ;
    skos:example "App zum systematischen Bibelstudium, Gebets-App mit Anleitungen"@de ;
    skos:broader <app> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/METHODISCHE_APP> ;
    skos:inScheme <scheme> .

<software> a skos:Concept ;
    skos:prefLabel "Desktop-Software"@de, "Desktop Software"@en ;
    skos:definition "Applikationen für Desktop-Computer im religionspädagogischen Kontext."@de ;
    skos:example "Bibelsoftware für Textanalyse, Religionspädagogische Planungssoftware"@de ;
    skos:broader <app> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/SOFTWARE> ;
    skos:inScheme <scheme> .

<webtool> a skos:Concept ;
    skos:prefLabel "Webtool"@de, "Webtool"@en ;
    skos:definition "Browserbasierte Software für religionspädagogische Zwecke."@de ;
    skos:example "Online-Bibel-Lexikon, Webtool zur Erstellung von Kreuzworträtseln mit religiösen Begriffen"@de ;
    skos:broader <app> ;
    skos:exactMatch <https://vocabs.sodix.de/sodix/educational/learningresourcetype/WEBTOOL>, <https://bokahama.github.io/SCRP-Vocab-LRT/WEBTOOL> ;
    skos:inScheme <scheme> .

# ========================================
# AUFGABE UND ÜBUNG (Top-Level)
# ========================================

<aufgabe_uebung> a skos:Concept ;
    skos:topConceptOf <scheme> ;
    skos:prefLabel "Aufgaben und Übungen"@de, "Tasks and Exercises"@en ;
    skos:altLabel "Arbeitsblätter, Aufgaben und Übungen"@de ;
    skos:definition "Spezifische Aktivitäten im Religionsunterricht zum Erwerb von Wissen und Kompetenzen."@de ;
    skos:example "Bibelstellen-Zuordnung, Ethische Fallbeispiele bearbeiten"@de ;
    skos:narrower <arbeitsblatt>, <lernaufgabe>, <recherche_auftrag>, <uebung> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/AUFGABE_UEBUNG> ;
    skos:inScheme <scheme> .

<arbeitsblatt> a skos:Concept ;
    skos:prefLabel "Arbeitsblatt"@de, "Worksheet"@en ;
    skos:definition "Dokument mit Aufgaben zu religiösen Themen zum Üben und Vertiefen."@de ;
    skos:example "Arbeitsblatt 'Die Zehn Gebote', Lösungsblatt zu 'Gleichnisse Jesu'"@de ;
    skos:broader <aufgabe_uebung> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/worksheet>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/ARBEITSBLATT>, <https://bokahama.github.io/SCRP-Vocab-LRT/ARBEITSBLATT> ;
    skos:inScheme <scheme> .

<lernaufgabe> a skos:Concept ;
    skos:prefLabel "Lernaufgabe"@de, "Learning Task"@en ;
    skos:altLabel "Aufgabe"@de ;
    skos:definition "Spezifische Aufgabe zur Erreichung religiöser oder ethischer Lernziele."@de ;
    skos:example "Aufgabe 'Vergleiche Schöpfungsmythen', Recherche 'Religiöse Feste im Jahreskreis'"@de ;
    skos:broader <aufgabe_uebung> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/LERNAUFGABE> ;
    skos:inScheme <scheme> .

<recherche_auftrag> a skos:Concept ;
    skos:prefLabel "Recherche-Auftrag"@de, "Research Assignment"@en ;
    skos:definition "Aufgabe zur eigenständigen Recherche zu religiösen Themen."@de ;
    skos:example "Recherche 'Heilige Schriften der Weltreligionen', WebQuest zu 'Martin Luther'"@de ;
    skos:broader <aufgabe_uebung> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/questionnaire> ;
