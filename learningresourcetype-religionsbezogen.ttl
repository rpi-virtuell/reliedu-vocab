    skos:broader <religioeser_quellentext> ;
    skos:inScheme <scheme> .

<andere_heilige_schrift> a skos:Concept ;
    skos:prefLabel "Andere Heilige Schrift"@de, "Other Sacred Scripture"@en ;
    skos:definition "Heilige Schriften aus anderen religiösen Traditionen."@de ;
    skos:example "Bhagavad Gita (Hinduismus), Tripitaka (Buddhismus), Guru Granth Sahib (Sikhismus)"@de ;
    skos:broader <religioeser_quellentext> ;
    skos:inScheme <scheme> .

<theologischer_text> a skos:Concept ;
    skos:prefLabel "Theologischer Text"@de, "Theological Text"@en ;
    skos:altLabel "Theologische Schrift"@de ;
    skos:definition "Theologische oder religionsphilosophische Texte und Abhandlungen."@de ;
    skos:example "Augustinus 'Confessiones', Thomas von Aquin 'Summa Theologica', Barmer Theologische Erklärung"@de ;
    skos:broader <religioeser_quellentext> ;
    skos:inScheme <scheme> .

<kirchendokument> a skos:Concept ;
    skos:prefLabel "Kirchendokument"@de, "Church Document"@en ;
    skos:altLabel "Kirchliches Lehrschreiben"@de, "Konzilstext"@de ;
    skos:definition "Offizielle Dokumente kirchlicher oder religiöser Autoritäten."@de ;
    skos:example "Enzyklika 'Laudato Si', Texte des Zweiten Vatikanischen Konzils, Luthers 95 Thesen"@de ;
    skos:broader <religioeser_quellentext> ;
    skos:inScheme <scheme> .

# ========================================
# RELIGIÖSER PRAXISBEZUG (Top-Level - NEUE KATEGORIE)
# ========================================

<religioeser_praxisbezug> a skos:Concept ;
    skos:topConceptOf <scheme> ;
    skos:prefLabel "Religiöser Praxisbezug"@de, "Religious Practice"@en ;
    skos:altLabel "Religiöse Praxis"@de, "Liturgie"@de ;
    skos:definition "Materialien und Ressourcen, die religiöse Praxis, Rituale, Liturgie und spirituelle Übungen dokumentieren oder anleiten."@de ;
    skos:definition "Materials and resources that document or guide religious practice, rituals, liturgy and spiritual exercises."@en ;
    skos:example "Gottesdienstablauf, Gebetsanleitung, Beschreibung eines religiösen Festes"@de ;
    skos:narrower <gottesdienst>, <gebet>, <religioeser_brauch>, <sakrament>, <religioeser_festtag> ;
    skos:inScheme <scheme> .

<gottesdienst> a skos:Concept ;
    skos:prefLabel "Gottesdienst"@de, "Worship Service"@en ;
    skos:altLabel "Liturgie"@de, "Messe"@de ;
    skos:definition "Materialien zur Gestaltung, Durchführung oder Erklärung von Gottesdiensten und liturgischen Feiern."@de ;
    skos:example "Ablauf einer Eucharistiefeier, Gestaltung eines Schulgottesdienstes, Struktur des Freitagsgebets (Salat al-Jumu'ah)"@de ;
    skos:broader <religioeser_praxisbezug> ;
    skos:inScheme <scheme> .

<gebet> a skos:Concept ;
    skos:prefLabel "Gebet"@de, "Prayer"@en ;
    skos:altLabel "Gebetstext"@de ;
    skos:definition "Gebete, Gebetsanleitungen oder Materialien zum Thema Gebet aus verschiedenen Traditionen."@de ;
    skos:example "Vaterunser, Rosenkranzgebet, Schma Israel, Islamisches Pflichtgebet (Salat)"@de ;
    skos:broader <religioeser_praxisbezug> ;
    skos:inScheme <scheme> .

<religioeser_brauch> a skos:Concept ;
    skos:prefLabel "Religiöser Brauch"@de, "Religious Custom"@en ;
    skos:altLabel "Ritual"@de, "Tradition"@de ;
    skos:definition "Beschreibung oder Anleitung zu religiösen Bräuchen, Ritualen und Traditionen."@de ;
    skos:example "Beschreibung einer Taufe, Anleitung zu Pessach-Seder, Erklärung des Ramadan"@de ;
    skos:broader <religioeser_praxisbezug> ;
    skos:inScheme <scheme> .

<sakrament> a skos:Concept ;
    skos:prefLabel "Sakrament"@de, "Sacrament"@en ;
    skos:definition "Materialien zu den Sakramenten der christlichen Kirchen."@de ;
    skos:example "Erklärung der Taufe, Ablauf der Firmung, Bedeutung der Eucharistie"@de ;
    skos:broader <religioeser_praxisbezug> ;
    skos:inScheme <scheme> .

<religioeser_festtag> a skos:Concept ;
    skos:prefLabel "Religiöser Festtag"@de, "Religious Festival"@en ;
    skos:altLabel "Kirchenjahr"@de, "Religiöses Fest"@de ;
    skos:definition "Materialien zu religiösen Festen und Festzeiten im Jahreskreis verschiedener Religionen."@de ;
    skos:example "Weihnachten, Ostern, Pfingsten, Chanukka, Ramadan und Eid al-Fitr, Diwali"@de ;
    skos:broader <religioeser_praxisbezug> ;
    skos:inScheme <scheme> .

# ========================================
# SONSTIGES (Top-Level)
# ========================================

<sonstiges> a skos:Concept ;
    skos:topConceptOf <scheme> ;
    skos:prefLabel "Diverse Medien und Formate"@de, "Other"@en ;
    skos:definition "Residualkategorie für religiöse Lernressourcen, die keinem anderen Typ zugeordnet werden können."@de ;
    skos:example "Gemischte Materialsammlung, nicht eindeutig kategorisierbare Ressource"@de ;
    skos:narrower <fragebogen>, <kurs>, <modell>, <praesentation>, <daten>, <webseite>, <glossar> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/other>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/SONSTIGES>, <https://bokahama.github.io/SCRP-Vocab-LRT/SONSTIGES> ;
    skos:inScheme <scheme> .

<fragebogen> a skos:Concept ;
    skos:prefLabel "Fragebogen"@de, "Questionnaire"@en ;
    skos:altLabel "Umfrage"@de ;
    skos:definition "Instrument zur Datenerhebung oder Reflexion im Religionsunterricht."@de ;
    skos:example "Fragebogen zur religiösen Sozialisation, Umfrage zu ethischen Einstellungen"@de ;
    skos:broader <sonstiges> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/questionnaire> ;
    skos:exactMatch <https://bokahama.github.io/SCRP-Vocab-LRT/FRAGEBOGEN> ;
    skos:inScheme <scheme> .

<kurs> a skos:Concept ;
    skos:prefLabel "Kurs"@de, "Course"@en ;
    skos:definition "Mehrere Unterrichtseinheiten zu einem religiösen Thema über einen bestimmten Zeitraum."@de ;
    skos:example "Online-Kurs 'Weltreligionen', Glaubenskurs für Erwachsene"@de ;
    skos:broader <sonstiges> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/course>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/KURS>, <https://bokahama.github.io/SCRP-Vocab-LRT/KURS> ;
    skos:inScheme <scheme> .

<modell> a skos:Concept ;
    skos:prefLabel "Modell"@de, "Model"@en ;
    skos:definition "Modell zur Veranschaulichung religiöser Strukturen oder Gebäude."@de ;
    skos:example "3D-Modell des Jerusalemer Tempels, Modell einer Synagoge"@de ;
    skos:broader <sonstiges> ;
    skos:exactMatch <https://vocabs.sodix.de/sodix/educational/learningresourcetype/MODELL>, <https://bokahama.github.io/SCRP-Vocab-LRT/MODELL> ;
    skos:inScheme <scheme> .

<praesentation> a skos:Concept ;
    skos:prefLabel "Präsentation"@de, "Presentation"@en ;
    skos:definition "Visualisierendes Material zur Unterstützung eines Vortrags im Religionsunterricht."@de ;
    skos:example "PowerPoint-Präsentation 'Islam in Deutschland', Folien zu 'Leben und Wirken Martin Luthers'"@de ;
    skos:broader <sonstiges> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/slide>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/PRESENTATION>, <https://bokahama.github.io/SCRP-Vocab-LRT/PRAESENTATION> ;
    skos:inScheme <scheme> .

<daten> a skos:Concept ;
    skos:prefLabel "Tabelle / Daten"@de, "Data"@en ;
    skos:definition "Empirische Daten, Statistiken oder Tabellen zu religiösen Themen."@de ;
    skos:example "Statistik zur Religionszugehörigkeit, Tabelle religiöser Feiertage, Erhebung zu religiöser Praxis"@de ;
    skos:broader <sonstiges> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/data>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/DATEN>, <https://bokahama.github.io/SCRP-Vocab-LRT/DATEN> ;
    skos:inScheme <scheme> .

<webseite> a skos:Concept ;
    skos:prefLabel "Webseite"@de, "Website"@en ;
    skos:altLabel "Internetseite"@de ;
    skos:definition "Webdokument mit religiösen Inhalten, aufrufbar per URL."@de ;
    skos:example "Webseite mit Materialien für Religionsunterricht, Wiki zu Kirchengeschichte, Blog über interreligiösen Dialog"@de ;
    skos:broader <sonstiges> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/web_page>, <https://vocabs.sodix.de/sodix/educational/learningresourcetype/WEBSEITE>, <https://bokahama.github.io/SCRP-Vocab-LRT/WEBSEITE> ;
    skos:inScheme <scheme> .

<glossar> a skos:Concept ;
    skos:prefLabel "Glossar"@de, "Glossary"@en ;
    skos:altLabel "Nachschlagewerk"@de, "Lexikon"@de ;
    skos:definition "Alphabetisch oder thematisch sortierte Liste religiöser Begriffe mit Erklärungen."@de ;
    skos:example "Theologisches Fachglossar, Lexikon religiöser Symbole, Begriffslexikon Weltreligionen"@de ;
    skos:broader <sonstiges> ;
    skos:relatedMatch <https://w3id.org/kim/hcrt/index> ;
    skos:exactMatch <https://vocabs.sodix.de/sodix/educational/learningresourcetype/GLOSSAR>, <https://bokahama.github.io/SCRP-Vocab-LRT/GLOSSAR> ;
    skos:inScheme <scheme> .
